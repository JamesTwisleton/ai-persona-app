{
  "version": 2,
  "builds": [
    {
      "src": "code/nextjs/next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "code/nextjs/app/api/$1"
    },
    {
      "src": "/api/p/(.*)",
      "dest": "code/nextjs/app/api/p/$1"
    },
    {
      "src": "/(.*)",
      "dest": "code/nextjs/app/$1"
    },
    {
      "src": "/p/(.*)",
      "dest": "code/nextjs/app/p/$1"
    }
  ]
}
